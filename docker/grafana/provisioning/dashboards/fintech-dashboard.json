{
  "dashboard": {
    "id": null,
    "title": "Fintech Real-time Analytics",
    "tags": ["fintech", "real-time", "analytics"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Fraud Detection Alerts",
        "type": "stat",
        "targets": [
          {
            "refId": "A",
            "expr": "fintech.customer_fraud_alerts",
            "legendFormat": "Fraud Alerts"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 1}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Transaction Metrics",
        "type": "timeseries",
        "targets": [
          {
            "refId": "A",
            "expr": "fintech.customer_transaction_metrics",
            "legendFormat": "Transaction Metrics"
          }
        ]
      },
      {
        "id": 3,
        "title": "Risk Analytics",
        "type": "gauge",
        "targets": [
          {
            "refId": "A",
            "expr": "fintech.customer_lifecycle",
            "legendFormat": "Customer Lifecycle"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 80}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Merchant Performance",
        "type": "timeseries",
        "targets": [
          {
            "refId": "A",
            "expr": "fintech.merchant_performance",
            "legendFormat": "Merchant Performance"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}
